{
  "name": "@ilyasemenov/eslint-config",
  "type": "module",
  "version": "1.4.1",
  "description": "Opinionated ESLint config on top of @antfu/eslint-config",
  "author": "Ilya Semenov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IlyaSemenov/lint-config"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "!src/**/*.test.*",
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "test": "vitest run"
  },
  "peerDependencies": {
    "eslint": "^9.5.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "eslint-plugin-annotation": "^1.1.6",
    "eslint-plugin-perfectionist": "^4.11.0",
    "eslint-plugin-vue-pug": "^1.0.0-alpha.2"
  },
  "devDependencies": {
    "tsconfig-vite-node": "^1.1.2",
    "tsup": "^8.4.0",
    "vitest": "^3.1.1"
  }
}
